<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="untref.tesis.gio">

    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE"/>
    <uses-permission android:name="android.permission.ACCESS_WIFI_STATE"/>
    <uses-permission android:name="android.permission.INTERNET"/>

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">

        <activity android:name=".presentation.activity.DefaultLoginActivity" android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity android:name=".presentation.activity.DefaultCreateUserActivity" android:screenOrientation="portrait"/>
        <activity android:name=".presentation.activity.DefaultDashboardActivity" android:screenOrientation="portrait"/>
        <activity android:name=".presentation.activity.DefaultCreateTermDepositActivity" android:screenOrientation="portrait"/>
    </application>

</manifest>
